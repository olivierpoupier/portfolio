---
import BaseHead from '../components/BaseHead.astro';
import Footer from '../components/Footer.astro';
import Header from '../components/Header.astro';
import { SITE_TITLE } from '../consts';
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={`Case Studies | ${SITE_TITLE}`} description="My portfolio of work and case studies" />
	</head>
	<body>
		<Header />
		<main>
			<h1>Case Studies</h1>
			<p>Explore my portfolio of projects and case studies.</p>
		</main>
		<Footer />
	</body>
</html>